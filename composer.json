{
    "name": "svendeka/theme",
    "description": "A starting point for TYPO3 projects",
    "homepage": "https://team23.de",
    "license": ["GPL-2.0+"],
    "type": "typo3-cms-extension",
    "version": "1.0.0",
    "keywords": [
        "TYPO3 CMS theming"
     ],
    "require": {
        "typo3/minimal": "^10.4"
    },
    "replace": {
        "svendeka-theme": "self.version",
        "typo3-ter/svendeka-theme": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "theme"
        }
    },
    "autoload": {
        "psr-4": {
            "Svendeka\\Theme\\": "Classes/"
        }
    }

}
